@model IList<XingZen.Domain.Model.Store>
@{
    ViewData["Title"] = "Register";
}

<h1>The page</h1>

<div>    
UserClaims: @ViewData["UserClaims"]
</div>

<div>    
Message: @ViewData["Message"]
</div>


@foreach (var store in Model) {
    <div>
        <h3>
            @store.Name 
            <small>@store.Id</small>
        </h3>
            <a asp-action="Display" asp-route-id="@store.Id">Display</a>
            <a asp-action="Numpad" asp-route-id="@store.Id">Numpad</a>
    </div>
} 


<div>
    <h2>Create new store</h2>
    <form action="/CashRegister/createStore" method="POST">
    <input type="text" name="name"/>
    <input type="submit" />
    </form>
</div>